{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/contatos.js"
    }
  ],
  "env": {
    "FIREBASE_API_KEY": "@your-firebase-api-key",
    "FIREBASE_AUTH_DOMAIN": "@your-firebase-domain"
  }
}