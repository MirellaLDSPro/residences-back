{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/contatos.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Authorization, Content-Type"
        }
      ]
    }],
    "build": {
      "env": {
        "FIREBASE_MESSAGING_SENDER_ID": "valor_teste"
      }
    },
    "env": {
      "FIREBASE_API_KEY": "FIREBASE_API_KEY",
      "FIREBASE_AUTH_DOMAIN": "FIREBASE_AUTH_DOMAIN",
      "NODE_ENV": "NODE_ENV",
      "FIREBASE_ADMIN_SDK_PATH": "FIREBASE_ADMIN_SDK_PATH",
      "GOOGLE_APPLICATION_CREDENTIALS": "GOOGLE_APPLICATION_CREDENTIALS",
      "ADMIN_USER_ID": "ADMIN_USER_ID",
      "FIREBASE_MESSAGING_SENDER_ID": "FIREBASE_MESSAGING_SENDER_ID",
      "FIREBASE_STORAGE_BUCKET": "FIREBASE_STORAGE_BUCKET",
      "FIREBASE_PROJECT_ID": "FIREBASE_PROJECT_ID",
      "FIREBASE_APP_ID": "FIREBASE_APP_ID",
      "FIREBASE_ADMIN_TYPE": "service_account",
      "FIREBASE_ADMIN_PRIVATE_KEY_ID": "FIREBASE_ADMIN_PRIVATE_KEY_ID",
      "FIREBASE_ADMIN_PRIVATE_KEY": "FIREBASE_ADMIN_PRIVATE_KEY",
      "FIREBASE_ADMIN_CLIENT_EMAIL": "FIREBASE_ADMIN_CLIENT_EMAIL",
      "CLIENTE_ID": "CLIENTE_ID"
    }
  }